Proceso chequeo_mayorque
	Definir n1, n2 Como Real;
	Escribir "Dime un primer número";
	Leer n1;
	Escribir "Dime un segundo número";
	Leer n2;
	Si n1 > n2 y n1 <> 0 y n2 <> 0 Entonces
		Escribir "El primer número es mayor que el segundo";
	SiNo
		Escribir "El segundo número es mayor que el primero";
	FinSi
FinProceso
