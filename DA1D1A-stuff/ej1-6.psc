Proceso media_tres_numeros
	Definir numero1, numero2, numero3, mediatotal Como Real;
	Escribir "Dime el primer número";
	Leer numero1;
	Escribir "Dime el segundo número";
	Leer numero2;
	Escribir "Dime el tercer número";
	Leer numero3;
	mediatotal = (numero1 + numero2 + numero3) / 3;
	Escribir "La media de los 3 números es: ", mediatotal;
